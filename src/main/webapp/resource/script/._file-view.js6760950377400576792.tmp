//判断是哪种浏览方式在网页中呈现
	var viewJudge = {
		//office类型，用pdf呈现
		pdf : function(ext) {
			if (ext == "doc" || ext == "docx" || ext == "xls" || ext == "xlsx" || ext == "ppt" || ext == "pptx" || ext == "pdf") {
				return true;
			}
			else {
				return false;
			}
		},
		//图片类型
		jpg : function(ext) {
			if (ext == "png" || ext == "gif" || ext == "jpg" || ext == "jpeg" || ext == "bmp" || ext == "pptx" || ext == "pdf") {
				return true;
			}
			else {
				return false;
			}
		},
		//视频类型
		flv : function(ext) {
			if (ext == "mp4" || ext == "avi" || ext == "flv" || ext == "rmvb" || ext == "wmv" || ext == "mkv") {
				return true;
			}
			else {
				return false;
			}
		},
		txt : function(ext) {
			if (ext == "txt") {
				return true;
			}
			else {
				return false;
			}
		},
		mp3 : function(ext) {
			if (ext == "mp3") {
				return true;
			}
			else {
				return false;
			}
		}
	}

	